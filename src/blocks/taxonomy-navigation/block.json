{
  "apiVersion": 3,
  "name": "we-taxonomy-navigation/taxonomy-navigation",
  "title": "Taxonomy Navigation",
  "category": "widgets",
  "icon": "menu",
  "description": "Navigation block that renders taxonomy terms.",
  "textdomain": "we-taxonomy-navigation",
  "attributes": {
    "taxonomy": {
      "type": "string",
      "default": "category"
    },
    "includeEmpty": {
      "type": "boolean",
      "default": true
    },
    "orderBy": {
      "type": "string",
      "default": "name"
    },
    "order": {
      "type": "string",
      "default": "ASC"
    },
    "showHierarchy": {
      "type": "boolean",
      "default": true
    },
    "maxDepth": {
      "type": "number",
      "default": 0
    }
  },
  "supports": {
    "anchor": true,
    "align": true,
    "color": {
      "background": true
    },
    "spacing": {
      "margin": true,
      "padding": true
    },
    "layout": false
  },
  "editorScript": "file:./index.js"
}
